{
    INLET_JET
    {
        type            fixedValue;
        value           uniform 0.1318;
    }
    INLET_SLOT_1
    {
        type            fixedValue;
        value           uniform 0;
    }
    INLET_SLOT_2
    {
        type            fixedValue;
        value           uniform 0.0446;
    }
    INLET_COFLOW
    {
        type            fixedValue;
        value           uniform 0;
    }
    WALL_BB_1
    {
        type            zeroGradient;
    }
    WALL_BB_2
    {
        type            zeroGradient;
    }
    SIDE
    {
        type            zeroGradient;
    }
    OUTLET
    {
        type            zeroGradient;
    }
    BACK
    {
        type            cyclic;
    }
    FRONT
    {
        type            cyclic;
    }
    AXIS
    {
        type            symmetry;
    }
