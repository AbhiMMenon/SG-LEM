FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleFace;
}

interpolationScheme cell;
enabled         false;
outputControl   timeStep;
writeInterval   1e-6;

fields
(
    U rho T p Cmean T_LEM CO2 //prod_CO prod_OH
);

